          тупысе(A[i,j]," ");
      циа-текос
	  тупысе(EOLN);
   циа-текос 

	тупысе(EOLN,"пимайас B",EOLN);
   циа i:=1 еыс N епамакабе
      циа j:=1 еыс N епамакабе          
          тупысе(B[i,j]," ");
      циа-текос
	  тупысе(EOLN);
   циа-текос    
   
	тупысе(EOLN,"цимолемо тым пимайым",EOLN);
   циа i:=1 еыс N епамакабе
