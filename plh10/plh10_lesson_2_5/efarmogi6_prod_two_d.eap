	циа i:=1 еыс N епамакабе
		циа j:=1 еыс N епамакабе
			тупысе("дЧСЕ ТО СТОИВЕъО ТОУ ПъМАЙА B[", i, ",",j,"]:");
			диабасе(B[i,j]);
		циа-текос
	циа-текос
	
   	/* уПОКОЦИСЛЭР цИМОЛщМОУ */

	циа i:=1 еыс N епамакабе
		циа j:=1 еыс N епамакабе
			sum:=0;
			циа k:=1 еыс N епамакабе
				sum:=sum+A[i,k]*B[k,j];
			циа-текос
			C[i,j]:=sum;
		циа-текос	
	циа-текос	
	
	
	
	
	/* еЙТЩПЫСГ АПОТЕКЕСЛэТЫМ */
	тупысе(EOLN,"пимайас а",EOLN);
   циа i:=1 еыс N епамакабе
